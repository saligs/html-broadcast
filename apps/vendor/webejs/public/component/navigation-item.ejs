<% if (typeof childs !== 'undefined' && childs) { %>
<% var tmp=false; %>
<% _.each(childs, function(e, i){ %>
<% if(e.name == window._wiz_global.configPage.name){ %>
    <% tmp=true; %>
<% } %>
<% }); %>
<% if(name == window._wiz_global.configPage.name || tmp){ %>
<li class="active open">
<% }else{ %>
<li>
<% } %>
    <a href="javascript:void(0)">
        <div class="item-content">
            <div class="item-media">
                <i class="fa fa-clone"></i>
            </div>
            <div class="item-inner">
                <span class="title"> <%= label %> </span><i class="icon-arrow"></i>
            </div>
        </div> </a>
    <ul class="sub-menu">
        <% _.each(childs, function(e, i){ %>
            <li>
                <a href="<%= e.href %>"> <span class="title"> <%= e.label %> </span> </a>
            </li>
        <% }); %>
    </ul>
</li>
<% }else{ %>
    <% if(name == window._wiz_global.configPage.name){ %>
<li class="active open">
    <% }else{ %>
<li>
    <% } %>
    <a href="<%= href %>">
        <div class="item-content">
            <div class="item-media">
                <i class="<%= icon %>"></i>
            </div>
            <div class="item-inner">
                <span class="title"> <%= label %> </span>
            </div>
        </div> </a>
</li>
<% } %>


